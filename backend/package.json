{
  "name": "pixelbuddy-backend",
  "version": "1.0.0",
  "description": "PixelBuddy Backend API - Kids-friendly AI pixel art studio",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.1.0",
    "@fastify/multipart": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^7.0.0",
    "fastify": "^4.24.0",
    "fastify-plugin": "^4.5.0",
    "drizzle-orm": "^0.29.0",
    "postgres": "^3.4.0",
    "redis": "^4.6.0",
    "zod": "^3.22.0",
    "openai": "^4.20.0",
    "sharp": "^0.32.0",
    "@napi-rs/canvas": "^0.1.44",
    "gif-encoder-2": "^1.0.5",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "drizzle-kit": "^0.20.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "keywords": [
    "pixel-art",
    "ai",
    "kids",
    "fastify",
    "image-processing",
    "gif"
  ],
  "author": "PixelBuddy Team",
  "license": "MIT"
}